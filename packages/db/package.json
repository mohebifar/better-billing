{
  "name": "@better-billing/db",
  "version": "0.1.0",
  "description": "Database layer for Better Billing",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./adapters/drizzle": {
      "types": "./dist/adapters/drizzle.d.ts",
      "import": "./dist/adapters/drizzle.mjs",
      "require": "./dist/adapters/drizzle.cjs"
    },
    "./generators/drizzle": {
      "types": "./dist/generators/drizzle.d.ts",
      "import": "./dist/generators/drizzle.mjs",
      "require": "./dist/generators/drizzle.cjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepack": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/"
  },
  "keywords": [
    "better-billing"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohebifar/better-billing.git",
    "directory": "packages/db"
  },
  "homepage": "https://github.com/mohebifar/better-billing#readme",
  "bugs": {
    "url": "https://github.com/mohebifar/better-billing/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@electric-sql/pglite": "catalog:dev-db-adapters",
    "@prisma/client": "catalog:dev-db-adapters",
    "@types/node": "catalog:",
    "@types/pg": "catalog:dev-db-adapters",
    "drizzle-kit": "catalog:dev-db-adapters",
    "drizzle-orm": "catalog:dev-db-adapters",
    "prisma": "catalog:dev-db-adapters",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "better-call": "^1.0.16",
    "zod": "^4.1.4"
  },
  "peerDependencies": {
    "@prisma/client": ">=5.0.0",
    "drizzle-orm": ">=0.29.0"
  },
  "peerDependenciesMeta": {
    "@prisma/client": {
      "optional": true
    },
    "drizzle-orm": {
      "optional": true
    }
  },
  "files": [
    "dist"
  ]
}
