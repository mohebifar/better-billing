{
  "name": "better-billing",
  "version": "0.1.1-alpha.2",
  "description": "TypeScript-first, provider-agnostic billing infrastructure",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./adapters/drizzle": {
      "types": "./dist/adapters/drizzle.d.ts",
      "import": "./dist/adapters/drizzle.mjs",
      "require": "./dist/adapters/drizzle.cjs"
    },
    "./adapters/prisma": {
      "types": "./dist/adapters/prisma.d.ts",
      "import": "./dist/adapters/prisma.mjs",
      "require": "./dist/adapters/prisma.cjs"
    },
    "integrations/next-js": {
      "types": "./dist/integrations/next-js.d.ts",
      "import": "./dist/integrations/next-js.mjs",
      "require": "./dist/integrations/next-js.cjs"
    },
    "integrations/node": {
      "types": "./dist/integrations/node.d.ts",
      "import": "./dist/integrations/node.mjs",
      "require": "./dist/integrations/node.cjs"
    },
    "integrations/react-start": {
      "types": "./dist/integrations/react-start.d.ts",
      "import": "./dist/integrations/react-start.mjs",
      "require": "./dist/integrations/react-start.cjs"
    },
    "integrations/solid-start": {
      "types": "./dist/integrations/solid-start.d.ts",
      "import": "./dist/integrations/solid-start.mjs",
      "require": "./dist/integrations/solid-start.cjs"
    },
    "integrations/svelte-kit": {
      "types": "./dist/integrations/svelte-kit.d.ts",
      "import": "./dist/integrations/svelte-kit.mjs",
      "require": "./dist/integrations/svelte-kit.cjs"
    },
    "./plugins/core": {
      "types": "./dist/plugins/core/index.d.ts",
      "import": "./dist/plugins/core/index.mjs",
      "require": "./dist/plugins/core/index.cjs"
    },
    "./plugins/stripe": {
      "types": "./dist/plugins/stripe/index.d.ts",
      "import": "./dist/plugins/stripe/index.mjs",
      "require": "./dist/plugins/stripe/index.cjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepack": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/",
    "vitest": "vitest"
  },
  "keywords": [
    "billing",
    "subscription",
    "typescript",
    "stripe",
    "saas"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohebifar/better-billing.git",
    "directory": "packages/better-billing"
  },
  "homepage": "https://github.com/mohebifar/better-billing#readme",
  "bugs": {
    "url": "https://github.com/mohebifar/better-billing/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@electric-sql/pglite": "catalog:dev-db-adapters",
    "@types/node": "catalog:",
    "drizzle-orm": "catalog:dev-db-adapters",
    "stripe": "^18.4.0",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "@better-billing/db": "workspace:*",
    "better-call": "^1.0.16",
    "zod": "^4.1.4"
  },
  "files": [
    "dist"
  ]
}
